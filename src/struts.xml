<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<constant name="struts.devMode" value="true" />	
	 <package name="default" extends="struts-default">
     <action name="checkAdmin" class="controller.CheckAdmin">
       <result>/login_admin_suc.jsp</result>
       <result name="fail">/login_admin_fail.jsp</result>
       <result name="input">/login_admin.jsp</result>
     </action>
     <action name="rentHouse" class="controller.RentHouse">
       <result>/show_house.jsp</result>
       <result name="fail">/login_admin_fail.jsp</result>
       <result name="input">/login_admin.jsp</result>
     </action>
     
     <action name="collect" class="controller.RentHouse" method="collect">
       <result>/collect_suc.jsp</result>
       <result name="fail">/login_admin_fail.jsp</result>
       <result name="input">/login_admin.jsp</result>
     </action>
     
     <action name="myCollection" class="controller.RentHouse" method="myCollection">
       <result>/myCollection.jsp</result>
       <result name="fail">/login_admin_fail.jsp</result>
       <result name="input">/login_admin.jsp</result>
     </action>
     
      <action name="addHouse" class="controller.RentHouse" method="addHouse">
       <result>/house_add_suc.jsp</result>
      
     </action>
     
      <action name="checkUser" class="controller.CheckUser">
       <result>/login_user_suc.jsp</result>
       <result name="fail" >/login_user_fail.jsp</result>
       <result name="input">/login_user.jsp</result>
     </action>
     <action name="addUser" class="controller.CheckUser" method="addUser">
       <result>/user_add_suc.jsp</result>
       <result name="fail" >/login_user_fail.jsp</result>
       <result name="input">/login_user.jsp</result>
     </action>
    <action name="showPlace" class="controller.ShowPlace">
       <result>/show_place.jsp</result>
       <result name="fail">/login_admin_fail.jsp</result>
       <result name="input">/login_admin.jsp</result>
     </action>
  </package>
</struts>